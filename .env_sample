# .env_sample
# This file provides a template for required environment variables.
# Copy it to .env and fill in the actual values before running docker-compose.

# SQLite database URL (default works for local development)
DATABASE_URL=sqlite:///./test.db

# Google Gemini / Vertex AI API key â€“ replace with your actual key.
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY
GOOGLE_GENAI_USE_VERTEXAI=0

# OpenAI API key for GPT-4o (Multi-Model Judge Panel)
OPENAI_API_KEY=YOUR_OPENAI_API_KEY

# Anthropic API key for Claude 3.5 Sonnet (Multi-Model Judge Panel)
ANTHROPIC_API_KEY=YOUR_ANTHROPIC_API_KEY

# Weights & Biases Configuration (for MCP integration)
WANDB_API_KEY=YOUR_WANDB_API_KEY
WANDB_PROJECT=agent-store-sandbox
WANDB_ENTITY=your-entity
WANDB_BASE_URL=https://api.wandb.ai

# Security Gate Configuration
SECURITY_GATE_MAX_PROMPTS=10          # Maximum number of security prompts to execute (default: 10, production: 50-100)
SECURITY_GATE_TIMEOUT=10.0            # Timeout in seconds for each prompt execution
SECURITY_GATE_THROTTLE_SECONDS=1.0    # Wait time between prompts (rate limit protection)
ADVBENCH_MAX_SAMPLES=10               # Number of samples from AdvBench dataset
